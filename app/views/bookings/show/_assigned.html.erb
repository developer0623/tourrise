<form action="<%= unassign_employee_booking_path(@booking) %>" autocomplete="off" method="POST">
  <%= @booking.assignee %>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token -%>
  <input type="hidden" name="_method" value="patch">
  <span class="u-noWrap">
    (<button type="submit"><%= t("bookings.unassign") %></button>)
  </span>
</form>
