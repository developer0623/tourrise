<% breadcrumb :season_new, params[:product_id] %>

<div class="Content">
  <%= render 'partials/content_heading', title: t('seasons.new.title') %>

  <div class="Content-main">
    <%= form_with model: [@season.product, @season], html: { class: "Form", autocomplete: :off } do |f| %>
      <%= render partial: 'form', locals: { f: f } %>
    <% end %>
  </div>
</div>
